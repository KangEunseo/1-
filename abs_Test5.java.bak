abstract class Ship1{
	public abstract int maxspeed();
	public abstract int passenger();
}

class child_Ship1 extends Ship1{
	public int maxspeed(){
		return 100;
	}
	public int passenger(){
		return 203;
	}
}

public class abs_Test5{
	public static void main(String ar[]){
		child_Ship1 c=new child_Ship1();
		System.out.println("최대 속도 : "+c.maxspeed());
		System.out.println("탑승 인원 : "+c.passenger());
	}
}