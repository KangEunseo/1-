import java.util.*;

class Scan_hw4 {
 public static void main(String ar[]) {

	 Scanner sc=new Scanner(System.in);
	 
	 String s="ABC";
	 System.out.print("10 이하의 정수를 입력하시오. : ");
	 int n=sc.nextInt();

     if(n<=10) {
	 for(int i=1;i<=n;i++){
		 System.out.println(s);
	  }
	 }

	 else {
		 System.out.println("잘못 입력");
	 }
 }
}